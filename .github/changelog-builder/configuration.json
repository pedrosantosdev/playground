{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test"]
    },
    {
      "title": "## 💬 Other",
      "labels": ["other", "documentation", "docs"]
    },
    {
      "title": "## 📦 Dependencies",
      "labels": ["dependencies", "deps", "chore"]
    }
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
  "empty_template": "- no changes",
  "max_pull_requests": 1000,
  "max_back_track_time_days": 1000
}
